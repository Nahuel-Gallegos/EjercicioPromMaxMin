#include "arrays.h"
#define CantidadEdades 5

/*
 * hacer una funcion
 * en el main pedir una cantidad de edades
 * maximo
 * minimo
 * promedio
 */
int utn_maximoArrayInt(int* edadesArray,int size,int* resultado);
int main(void)
{
	int edadesArray[5];
	int resultado;

	edadesArray[0] = 32;
	edadesArray[1] = 50;
	edadesArray[2] = 15;
	edadesArray[3] = 80;
	edadesArray[4] = 27;

	utn_maximoArrayInt(edadesArray,CantidadEdades,&resultado);
